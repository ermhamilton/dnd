<!-- Scaffolding generated by Casein v5.1.1.5 -->


<div class="row">
	<div class="col-lg-6">
		<%= casein_text_field f, f.object, :name %>
	</div>
	<div class="col-lg-6">
		<%= casein_check_box f, f.object, :caster %>
	</div>
</div>

<div class="row">
	<div class="col-lg-6">
		<%= f.label "Ability Type" %><br />
		<%= f.collection_check_boxes :ability_ids, Ability.all, :id, :name do |b| %>
			<div class="collection-check-box">
		      <%= b.check_box %>
		      <%= b.label %>
		    </div>
		<% end %>
	</div>
	<div class="col-lg-6">
		<%= f.label "Skill Proficiencies" %><br />
